
@{
    //ViewBag.Title = "AltaLibro";
    Layout = "~/Views/Shared/_Layout.cshtml";
    TempData.Keep("adminEmail");
}

@if (ViewBag.AfectedRows == 1)
{
    <script type="text/javascript">
        alert("El libro ah sido dado de alta!");
        var url = '@Url.Action("Libros", "Home")';
        window.location.href = url;
    </script>
}
else
{
    <script type="text/javascript">
        alert("Error, el libro no se pudo dar de alta!");
        var url = '@Url.Action("Libros", "Home")';
        window.location.href = url;
    </script>
}

